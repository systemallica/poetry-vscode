{
	"name": "poetry-vscode",
	"displayName": "Poetry VS Code",
	"description": "Poetry support for VS Code",
	"version": "0.0.1",
	"publisher": "systemallica",
	"repository": "https://github.com/systemallica/poetry-vscode",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.poetryVscode",
				"title": "Check poetry-vscode status"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^14.14.19",
		"@types/node-fetch": "^2.5.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.11.1",
		"@typescript-eslint/parser": "^4.11.1",
		"eslint": "^7.17.0",
		"typescript": "^4.1.3"
	},
	"dependencies": {
		"node-fetch": "^2.6.1"
	}
}
